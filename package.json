{
  "name": "spa_boilerplate",
  "version": "1.0.0",
  "description": "Express server serving a react client via webpack middleware.",
  "main": "/src/express/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.jsx; exit 0",
    "wp-dev": "cd src/webpack && webpack-dev-server --open --config dev.js",
    "server": "nodemon src/express/index.js",
    "dev": "concurrently \"npm run server\" \"npm run wp-dev\"",
    "build": "cd src/webpack && webpack --config prod.js",
    "start": "node src/express/index.js",
    "demo": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nameer-rizvi/spa_boilerplate.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "redux",
    "webpack",
    "babel",
    "spa"
  ],
  "author": "Nameer Rizvi <rizvinameer@gmail.com> (https://github.com/nameer-rizvi)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nameer-rizvi/spa_boilerplate/issues"
  },
